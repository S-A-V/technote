  "stylelint.configOverrides": {
    "extends": [
      "@maiscrm/stylelint-config-standard"
    ]
  },
  "stylelint.configBasedir": "/Users/Edison/Downloads/Workspace/mai/portal/cli",
  "stylelint.packageManager": "yarn",
  "stylelint.stylelintPath": "/Users/Edison/Downloads/Workspace/mai/portal/cli/node_modules/stylelint",
  "stylelint.validate": [
    "css",
    "less",
    "postcss",
    "sass",
    "scss",
    "styled-css",
    "sugarss",
